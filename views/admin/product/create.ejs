 <!-- Page-Title -->
<div class="row">
    <div class="col-sm-12">
        
        <h4 class="page-title" style="text-align: center;">Thêm mới giao diện</h4>
    </div>
</div>
<div class="row">
    <div class="col-lg-3"></div>
    <div class="col-lg-6">
        <div class="card-box">
            <form action="#" method="" data-parsley-validate novalidate>
                <div class="form-group">
                    <label for="userName">Ảnh</label>
                    <input type="file" name="" value="" placeholder="">
                </div>
                <div class="form-group">
                    <label for="userName">Tên*</label>
                    <input type="text" name="name" parsley-trigger="change" required
                           placeholder="Nhập tên" class="form-control" value="" id="">
                </div>
                <div class="form-group">
                    <label for="userName">Đường dẫn</label>
                    <input type="text" name="alias" value="" class="form-control" placeholder="">
                </div>
                <div class="form-group">
                    <label for="">Chọn dự án</label>                   
                    <select class="selectpicker" data-show-subtext="true" class="form-control" data-live-search="true">  <option>--Chọn dự án--</option>
                        <option data-subtext="Rep California">Tom Foolery</option>
                        <option data-subtext="Sen California">Bill Gordon</option>
                        <option data-subtext="Sen Massacusetts">Elizabeth Warren</option>
                        <option data-subtext="Rep Alabama">Mario Flores</option>
                        <option data-subtext="Rep Alaska">Don Young</option>
                        <option data-subtext="Rep California" disabled="disabled">Marvin Martinez</option>
                    </select>
                </div>     
                <div class="form-group">
                    <label for="userName">Số thứ tự</label>
                    <input type="number" name="stt" placeholder="" class="form-control" style="width: 100px" value="" id="">
                </div>
                <div class="form-group text-right m-b-0">
                    <button class="btn btn-primary waves-effect waves-light" type="submit">
                        Thêm
                    </button>
                    <button type="reset" class="btn btn-default waves-effect waves-light m-l-5">
                        Hủy
                    </button>
                </div>

            </form>
        </div>
    </div><!-- end col -->
    <div class="col-lg-3"></div>
    
</div>
<!-- end row -->